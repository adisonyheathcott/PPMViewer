{
	"name": "ppmviewer",
	"displayName": "PPMViewer",
	"description": "Portable Pixmap Format Viewer for Visual Studio Code",
	"version": "1.4.0",
	"publisher": "AYH",
	"repository": {
		"type": "git",
		"url": "https://github.com/adisonyheathcott/PPMViewer"
	},
	"author": {
		"name": "Adison Heathcott",
		"url": "https://github.com/adisonyheathcott"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ppmviewer.viewppm"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ppmviewer.viewppm",
				"title": "View PPM"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "ppmviewer.viewppm",
					"group": "view",
					"when": "resourceExtname == .ppm"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.9.6",
		"@types/vscode": "^1.60.0",
		"eslint": "^7.32.0",
		"@typescript-eslint/parser": "^4.31.2",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"glob": "^7.2.0",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"vscode-uri": "^3.0.2"
	}
}
